# 应用配置 (development开发环境/production生产环境)
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development/production

# IP 地理检测 API
IP_API_URL=<你的IP地理检测API URL>

# 全局语言(zh/en)
NEXT_PUBLIC_DEFAULT_LANGUAGE=zh/en

# Supabase 配置 (国际版)
NEXT_PUBLIC_SUPABASE_URL=<你的Supabase项目URL>
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=<你的Supabase可发布密钥>
SUPABASE_SERVICE_ROLE_KEY=<你的Supabase服务角色密钥>

# CloudBase 配置 (国内版)
WECHAT_CLOUDBASE_ID=<你的微信云开发环境ID>
CLOUDBASE_SECRET_ID=<你的云开发Secret ID>
CLOUDBASE_SECRET_KEY=<你的云开发Secret Key>

# 阿里千问 API配置
DASHSCOPE_API_KEY=<你的阿里千问API密钥>

# Mistral API配置
MISTRAL_API_KEY=<你的Mistral API密钥>